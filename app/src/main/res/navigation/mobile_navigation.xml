<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_mapa">

    <fragment
        android:id="@+id/navigation_mapa"
        android:name="es.kamikaze.app.ui.map.MapFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_map" />

    <fragment
        android:id="@+id/navigation_bolsa"
        android:name="es.kamikaze.app.ui.bolsa.BolsaFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_bolsa" />

    <fragment
        android:id="@+id/navigation_social"
        android:name="es.kamikaze.app.ui.social.SocialFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_social" />
</navigation>